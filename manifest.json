{
    "background": {
        "page": "background.html"
    },
    "name": "fetchPage",
    "manifest_version": 2,
    "version": "1.0",
    "description": "fetch data on page",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "bookmarks",
        "unlimitedStorage",
        "notifications",
        "http://ops.airizu.com/"
    ]
}